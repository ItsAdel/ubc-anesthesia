      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f3c8bf69-e198-46c7-8560-4b74372a04f2"
      }
    },
    "owner": "itsadel",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
